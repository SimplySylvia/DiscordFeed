{
  "name": "discordfeed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest --config jest.backend.config.js --coverage",
    "test:frontend": "jest --config jest.config.js --coverage",
    "auth:setup": "node e2e/auth-setup.js",
    "test:e2e": "node e2e/auth-setup.js && playwright test",
    "db:migrate:init": "dotenv -e .env.local -- prisma migrate dev --name init",
    "db:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "bull-board": "dotenv -e .env.local -- tsx scripts/bull-board.ts",
    "verify": "dotenv -e .env.local -- npx tsx lib/discord/messageAggregation.verify.ts",
    "worker": "dotenv -e .env.local -- tsx scripts/indexingWorker.ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@prisma/client": "^5.7.1",
    "bull": "^4.16.5",
    "discord.js": "^14.14.1",
    "ioredis": "^5.6.1",
    "jose": "^6.0.10",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@bull-board/api": "^6.9.2",
    "@bull-board/express": "^6.9.2",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.1.8",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}
